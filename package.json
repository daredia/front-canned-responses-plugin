{
  "name": "front-companion",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@frontapp/plugin-sdk": "^1.0.1",
    "express": "^4.17.1",
    "luxon": "^1.24.1",
    "node-env-run": "^4.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "HTTPS=true PORT=3000 react-scripts start",
    "server": "node-env-run server --exec nodemon",
